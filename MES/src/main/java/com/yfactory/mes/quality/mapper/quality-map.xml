<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfactory.mes.quality.mapper.quality-map">
	<resultMap type="com.yfactory.mes.quality.vo.ErrCodeVO"
		id="errCode">
		<!-- 불량코드 -->
		<result property="err_cd" column="ERR_CD" />
		<!-- 불량코드명 -->
		<result property="err_nm" column="ERR_NM" />
		<!-- 불량코드 상세내용 -->
		<result property="err_dtl" column="ERR_DTL" />
	</resultMap>

	<resultMap type="com.yfactory.mes.quality.vo.MtCheckVO"
		id="mtCheck">
		<!-- 발주상세코드 -->
		<result property="po_dtlcd" column="PO_DTLCD" />
		<!-- 자재코드 -->
		<result property="mtrl_cd" column="MTRL_CD" />
		<!-- 검사일 -->
		<result property="chk_dt" column="CHK_DT" />
		<!-- 합격량 -->
		<result property="chk_passqty" column="CHK_PASSQTY" />
		<!-- 불량량 -->
		<result property="chk_errqty" column="CHK_ERRQTY" />
		<!-- 담당자 -->
		<result property="chk_mngr" column="CHK_MNGR" />
		<!-- 검사여부 -->
		<result property="chk_insp" column="CHK_INSP" />
	</resultMap>

	<resultMap type="com.yfactory.mes.quality.vo.MtErrListVO"
		id="mtErrList">
		<!--발주상세코드 -->
		<result property="po_dtlcd" column="PO_DTLCD" />
		<!-- 자재코드 -->
		<result property="mtrl_cd" column="MRTL_CD" />
		<!-- 불량량 -->
		<result property="chk_errqty" column="CHK_ERRQTY" />
		<!-- 불량코드 -->
		<result property="err_cd" column="ERR_CD" />
	</resultMap>

	<resultMap type="com.yfactory.mes.quality.vo.ProdErrListVO"
		id="ProdErrList">
		<!-- 완제품LOT -->
		<result property="prod_lot" column="PROD_LOT" />
		<!-- 불량량 -->
		<result property="chk_errqty" column="CHK_ERRQTY" />
		<!-- 불량코드 -->
		<result property="err_cd" column="ERR_CD" />
	</resultMap>
</mapper>